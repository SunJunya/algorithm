//				}else if((int) Math.sqrt(i*j)*(int) Math.sqrt(i*j)==i*j){
//					count++;
//				}else{
//					double flag=2*Math.sqrt(i)*Math.sqrt(j);
//					int iflag=(int) (2*Math.sqrt(i)*Math.sqrt(j));				
//					if(flag==iflag  ){
//						count++;
//					}
//				}
//				
//			}
			;
		}
		System.out.println(count);
		
	}

}
